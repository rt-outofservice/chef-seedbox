client
dev tun
proto udp
remote <%= node['openvpn']['client']['server-ip'] %> <%= node['openvpn']['server']['port'] %>

resolv-retry infinite
nobind
user nobody
group nogroup
persist-key
persist-tun
ca ca.crt
cert client.crt
key client.key
ns-cert-type server
tls-auth ta.key 1
cipher AES-256-CBC
comp-lzo
;verb 3
redirect-gateway def1

;push route <%= node['openvpn']['server']['client-ip'] %> <%= node['openvpn']['server']['client-mask'] %>